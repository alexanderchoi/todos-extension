{
  "name": "Todo Tab",
  "description": "Todo list on every new tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Todo Tab"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}